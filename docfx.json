{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "bin/**/*.dll"
            ,"src/ExSpans/ExSpans.csproj"
          ],
          "src": ""
        }
      ],
      "dest": "api",
      "properties": {
        "TargetFramework": "net9.0",
        "DefineConstants": "NETCOREAPP3_0_OR_GREATER;NET5_0_OR_GREATE;NET6_0_OR_GREATE;NET7_0_OR_GREATE;NET8_0_OR_GREATER;NET9_0_OR_GREATER"
      },
      "includePrivateMembers": false,
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "noRestore": false,
      "namespaceLayout": "flattened",
      "memberLayout": "separatePages",
      "EnumSortOrder": "alphabetic",
      "allowCompilationErrors": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "../*.md",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ],
        "exclude": [ "**/doxyfiles/**"]
      }
    ],
    "output": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "modern"
    ],
    "postProcessors": [],
    "keepFileLink": false,
    "disableGitFeatures": false
  }
}